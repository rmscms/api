{
    "name": "rmscms/api",
    "description": "Shared API infrastructure for RMS packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RMS Team"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/sanctum": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "RMS\\Api\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMS\\Api\\ApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

